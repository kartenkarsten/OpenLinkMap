			</match>
			<match mode="or">
				<tag k="operator" v="(.*;|^)ÖBB(;.*|$)"/>
				<tag k="uic_ref" v="81[0-9]*"/>
			</match>
		</match>
		<find>
			<tag k="uic_ref" v="([0-9]*)" match_id="uic_ref"/>
		</find>
		<output>
